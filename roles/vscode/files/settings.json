{
    "terminal.integrated.fontFamily": "Roboto Mono for Powerline",
    "problems.decorations.enabled": false,
    "files.associations": {
        "*.htm": "twig",
        "*.config": "plaintext"
    },
    "python.formatting.provider": "yapf",
    "terminal.integrated.rendererType": "dom",
    "breadcrumbs.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "none",
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "explorer.confirmDragAndDrop": false,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.enableNeovim": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.minimap.maxColumn": 70,
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "yaml.schemas": {
        "file:///Users/gilcunha/.vscode/extensions/atlassian.atlascode-2.6.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.explorer.enabled": false,
    "atlascode.jira.explorer.showAssignedIssues": false,
    "atlascode.jira.hover.enabled": false,
    "atlascode.jira.explorer.showOpenIssues": false,
    "atlascode.jira.statusbar.enabled": false,
    "atlascode.jira.statusbar.showUser": false,
    "atlascode.jira.statusbar.showProduct": false,
    "atlascode.jira.todoIssues.enabled": false,
    "atlascode.bitbucket.pipelines.explorerEnabled": false,
    "atlascode.bitbucket.pipelines.monitorEnabled": false,
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify",
        "editor.formatOnSave": false
    },
    "git.autofetch": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "liquid.format": false,
    "editor.acceptSuggestionOnEnter": "off",
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[elixir]": {
        "editor.defaultFormatter": "JakeBecker.elixir-ls"
    },
    "editor.formatOnType": true,
    "editor.formatOnPaste": true,
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "[HTML (Eex)]": {
        "editor.defaultFormatter": "zerokol.vscode-eex-beautify"
    },
    "debug.console.fontSize": 11,
    "editor.fontFamily": "Roboto Mono for Powerline",
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "emmet.includeLanguages": {
        "HTML (Eex)": "html"
    },
    "editor.tabCompletion": "on",
    "emmet.triggerExpansionOnTab": true,
    "editor.formatOnSave": true,
    "prettier.packageManager": "yarn",
    "go.useLanguageServer": true,
    "terminal.integrated.fontSize": 11,
    "atlascode.jira.enabled": false,
    "prettier.requireConfig": true,
    "prettier.requirePragma": true,
    "html.format.enable": false,
    "files.maxMemoryForLargeFilesMB": 8000,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}